@model IEnumerable<Transcript_Repository.Models.Module>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Add New", "Add")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ModuleTitle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ModuleResult)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ModuleTrimester)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ModuleComment)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ModuleTitle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ModuleResult)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ModuleTrimester)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ModuleComment)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.Id })
        </td>
    </tr>
}

</table>
